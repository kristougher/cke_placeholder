{#
/**
 * @file
 * Template for the page the user is redirected to after submitting crop form.
 */
#}
<script>
  var library_item = window.parent.jQuery('[data-id="{{ fid }}"]');
  library_item.find('h6').text('{{ title }}');
  library_item.attr('data-caption', '{{ caption }}');

  var droptarget_item = window.parent.jQuery('#file-{{ fid }}');
  droptarget_item.text('{{ title }}');
  window.parent.EditEntityWidget.dialogDestroy();
</script>
