{#
/**
 * @file
 * Library item template, used on node form.
 */
#}
<div
  draggable="true"
  class="cke-placeholder-draggable"
  ondragstart="ckePlaceholder.dragStart(event, '{{ cke_placeholder_tag }}')"
  {% for key, value in data %}
    data-{{ key }}="{{ value }}"
  {% endfor %}
  >
  {% if image_url is not empty %}
    <span class="cke-placeholder-image-wrap">
      <img src="{{ image_url }}" />
    </span>
  {% endif %}
  <span class="cke-placeholder-description">
    {{ description }}
  </span>
</div>
